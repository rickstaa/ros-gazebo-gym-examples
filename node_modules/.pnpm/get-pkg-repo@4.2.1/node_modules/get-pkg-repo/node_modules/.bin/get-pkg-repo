#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/node_modules:/home/ricks/Development/work/node_modules:/home/ricks/Development/node_modules:/home/ricks/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/node_modules:/home/ricks/Development/work/node_modules:/home/ricks/Development/node_modules:/home/ricks/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli.js" "$@"
else
  exec node  "$basedir/../../src/cli.js" "$@"
fi
