#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/node_modules:/home/ricks/Development/work/node_modules:/home/ricks/Development/node_modules:/home/ricks/node_modules:/home/node_modules:/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-core@4.2.4/node_modules/conventional-changelog-core/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-core@4.2.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/node_modules:/home/ricks/Development/work/node_modules:/home/ricks/Development/node_modules:/home/ricks/node_modules:/home/node_modules:/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-core@4.2.4/node_modules/conventional-changelog-core/node_modules:/home/ricks/Development/work/ros_gazebo_gym_ws/src/ros-gazebo-gym-examples/node_modules/.pnpm/conventional-changelog-core@4.2.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/cli.js" "$@"
else
  exec node  "$basedir/../../../../../conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/cli.js" "$@"
fi
